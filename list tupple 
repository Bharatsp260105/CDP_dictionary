list1=[1,2,3]
print(list1)
list1.append('zeal')
list1.remove(1)
print(list1)

tup=(1,2,3,"zeal")
l=list(tup)
print(l)
l.append("python")
l.remove(1)
tup=tuple(l)
print(tup)
list=[11,22,33,45,66,24]
print(list)
tup=tuple(list)
print(tup)
set1=set(tup)
print(set1)
fruits=frozenset(["apple","banana"])
print(fruits)

print("apple"not in fruits)
print("mango" in fruits)

a=frozenset([1,2,3,4,5,6])
b=frozenset([5,6,7,8])
print(a|b)
print(a-b)
print(a&b)

print(b-a)
print(b&a)
print(b|a)
